{
  "schema": {
    "project_name": "lab",
    "container_alias": "",
    "image_name": ""
  },
  "components": {
    ".project_name": {
      "field_description": "Name of project in lab registry",
      "default_value": "",
      "max_length": 64,
      "must_not_contain": [ "[^\\w\\-_]" ],
      "field_metadata": {
        "cli_help": "name of project in lab registry",
        "cli_metavar": "project"
      }
    },
    ".container_alias": {
      "field_description": "Docker container alias to add to config",
      "default_value": "",
      "max_length": 64,
      "must_not_contain": [ "[^\\w\\-_]" ],
      "field_metadata": {
        "cli_flags": [ "--container" ],
        "cli_help": "alias for project's docker container",
        "cli_metavar": "ALIAS"
      }
    },
    ".image_name": {
      "field_description": "Docker image name to add to project config",
      "default_value": "",
      "max_length": 64,
      "must_not_contain": [ "[^\\w\\-_/]" ],
      "field_metadata": {
        "cli_flags": [ "--image" ],
        "cli_help": "name for project's docker image",
        "cli_metavar": "NAME"
      }
    }
  }
}